{
  "name": "startup-fe",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "lint-staged": {
    "**/*.(ts|tsx)": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.json": [
      "yarn translate",
      "pretty-quick --staged",
      "git add"
    ]
  },
  "scripts": {
    "dev": "ionic serve",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test:e2e": "npx playwright test",
    "test": "vitest --run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "yarn prettier -w ./src/** --loglevel silent",
    "prepare": "husky",
    "ionic:build": "ionic build",
    "ionic:info": "ionic info",
    "cap:add:android": "npx cap add android",
    "cap:add:ios": "npx cap add ios",
    "cap:sync": "npx cap sync",
    "cap:copy": "npx cap copy",
    "cap:open:android": "npx cap open android",
    "cap:open:ios": "npx cap open ios",
    "cap:build:android": "npx cap run android --target emulator",
    "cap:build:ios": "npx cap run ios --target simulator",
    "clean": "rm -rf node_modules && rm -rf dist && yarn install",
    "env:switch:dev": "./environment/env-switch.sh dev",
    "env:switch:prod": "./environment/env-switch.sh prod",
    "translate": "node translate.js",
    "docs": "cd docs && yarn install && yarn dev",
    "mobile:resources:android": "node environment/android/android-resources.js --android",
    "mobile:resources:ios": "node environment/android/android-resources.js --ios",
    "mobile:resources": "node environment/android/android-resources.js --all",
    "android:build:debug": "./environment/android/android-build.sh debug",
    "android:build:release": "./environment/android/android-build.sh release"
  },
  "dependencies": {
    "@capacitor/android": "6.1.2",
    "@capacitor/app": "6.0.1",
    "@capacitor/core": "6.1.2",
    "@capacitor/haptics": "6.0.1",
    "@capacitor/ios": "6.1.2",
    "@capacitor/keyboard": "6.0.2",
    "@capacitor/preferences": "^6.0.2",
    "@capacitor/status-bar": "6.0.1",
    "@hookform/resolvers": "^3.9.1",
    "@ionic/react": "^8.0.0",
    "@ionic/react-router": "^8.0.0",
    "@types/i18next": "^13.0.0",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@vitalets/google-translate-api": "^9.2.0",
    "cordova-res": "^0.15.4",
    "i18next": "^23.16.1",
    "i18next-browser-languagedetector": "^8.0.0",
    "ionicons": "^7.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.53.1",
    "react-i18next": "^15.0.3",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "zod": "^3.23.8",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@capacitor/cli": "6.1.2",
    "@playwright/test": "^1.48.2",
    "@testing-library/dom": ">=7.21.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^16.0.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.13",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.8.1",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "husky": "^9.1.6",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "pretty-quick": "^4.0.0",
    "simple-git": "^3.27.0",
    "terser": "^5.4.0",
    "typescript": "5.5.4",
    "vite": "~5.2.0",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^0.34.6"
  },
  "description": "An Ionic project"
}
